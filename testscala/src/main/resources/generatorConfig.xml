        <!--<property name="enableSubPackages" value="false"/>-->
        <!--</sqlMapGenerator>-->
        <!--<javaClientGenerator type="XMLMAPPER" targetPackage="ml.guxing.test.java.core.mapper"-->
        <!--targetProject="src/main/java">-->
        <!--<property name="enableSubPackages" value="false"/>-->
        <!--</javaClientGenerator>-->
        <table tableName="SM_USER"/>
        <table tableName="MM_FIELD"/>
        <table tableName="MM_TABLE"/>
        <table tableName="SM_GROUP"/>
        <table tableName="SM_USER"/>
        <table tableName="TM_COOLMENU"/>
    </context>
</generatorConfiguration>