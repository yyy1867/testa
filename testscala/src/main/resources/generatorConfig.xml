<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <!-- 本地数据库驱动的位置 -->
    <classPathEntry location="G:\app\tools\h2\bin\h2-1.4.196.jar"/>
    <context id="h2DataBase" targetRuntime="MyBatis3Simple" defaultModelType="flat">
        <!-- 生成的Java文件的编码 -->
        <property name="javaFileEncoding" value="UTF-8"/>
        <!-- 格式化java代码 -->
        <!--<property name="javaFormatter" value="org.mybatis.generator.api.dom.DefaultJavaFormatter"/>-->
        <!-- 格式化XML代码 -->
        <!--<property name="xmlFormatter" value="org.mybatis.generator.api.dom.DefaultXmlFormatter"/>-->

        <commentGenerator>
            <property name="suppressAllComments" value="true"/>
            <property name="suppressDate" value="true"/>
        </commentGenerator>
        <jdbcConnection driverClass="org.h2.Driver"
                        connectionURL="jdbc:h2:tcp://localhost/D:/source/testa/testa/src/database/pkdb" userId="sa"/>
        <javaTypeResolver>
            <property name="forceBigDecimals" value="false"/>
        </javaTypeResolver>
        <javaModelGenerator targetPackage="ml.guxing.test.java.core.domain"
                            targetProject="src/main/java">
            <property name="enableSubPackages" value="false"/>
            <!--<property name="rootClass" value="ml.guxing.test.java.core.domain.model.SystemModel"/>-->
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>
